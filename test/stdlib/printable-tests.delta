// RUN: check_exit_status 0 %delta run -Werror %s

def main() {
    testCustomPrintablestruct();
}

struct A: Printable {
    var a: StringRef;
    var b: StringRef;

    init(a: StringRef, b: StringRef) {
        this.a = a;
        this.b = b;
    }

    def printTo(stream: mutable String*) {
        stream.write(a);
        stream.write("+");
        stream.write(b);
    }
}

def testCustomPrintablestruct() {
    assert(StringRef(A("foo", "bar").toString()) == "foo+bar");
}
