// RUN: check_exit_status 0 compile_and_run -Werror %s

def main() {
    testInsertionSort();
    testQuickSort();
    testMax();
}

def testInsertionSort() {
    var a = Array<int>();

    for (var i in 0..5) {

        a.push(5 - i);
    }

    var real = Array<int>();

    for (var i in 0..5) {
        real.push(i+1);
    }

    sort(&a);

    assert(ArrayRef(&a) == ArrayRef(&real));
}


def testQuickSort() {
    var a = Array<int>();

    for (var i in 0..64) {

        a.push(64 - i);
    }

    var real = Array<int>();

    for (var i in 0..64) {
        real.push(i+1);
    }

    sort(&a);

    assert(ArrayRef(&a) == ArrayRef(&real));
}

def testMax() {
    var a = 7;
    var b = 9;

    assert(*max(a, b) == 9);
}
