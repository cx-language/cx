// RUN: check_exit_status 0 %delta run -Werror %s

def main() {
    testPushPop();
    testEmpty();
    testPushAfterPop();
    testFirst();
}

def testPushPop() {
    var q = Queue<int>();

    assert(q.size() == 0);
    q.push(23);
    q.push(48);

    assert(q.size() == 2);

    var elem = q.pop();
    assert(q.size() == 1);
    assert(elem == 23);

    elem = q.pop();

    assert(elem == 48);
    assert(q.size() == 0);
}

def testEmpty() {
    var q = Queue<StringRef>();

    assert(q.isEmpty());

    q.push("foo");
    assert(!q.isEmpty());
    q.push("bar");
    assert(!q.isEmpty());
    q.pop();
    q.pop();
    assert(q.isEmpty());
}

def testPushAfterPop() {
    var q = Queue<StringRef>();

    q.push("foo");
    assert(q.size() == 1);
    q.pop();
    assert(q.size() == 0);
    q.push("bar");
    assert(q.size() == 1);
    q.pop();
    assert(q.size() == 0);
}

def testFirst() {
    var q = Queue<StringRef>();

    q.push("foo");
    q.push("bar");
    assert(*q.first() == "foo");
    assert(q.pop() == "foo");
    assert(*q.first() == "bar");
    assert(q.pop() == "bar");
}
