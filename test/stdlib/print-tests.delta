// RUN: %delta run -Werror %s | %FileCheck %s

class A: Printable {
    let a: StringRef;

    init(a: StringRef) {
        this.a = a;
    }

    func printTo(stream: mutable String*) {
        stream.write(a);
        stream.write(a);
        stream.write("!");
    }
}


func main() {
    // CHECK: foofoo!
    print(A("foo"));
}
