// RUN: check_exit_status 0 %delta run -Werror %s

void main() {
    assert('a'.isAlpha());
    assert('z'.isAlpha());
    assert('A'.isAlpha());
    assert('Z'.isAlpha());
    assert(!'_'.isAlpha());

    assert('0'.isDigit());
    assert('9'.isDigit());
    assert(!'A'.isDigit());

    assert(' '.isWhitespace());
    assert('\t'.isWhitespace());
    assert('\n'.isWhitespace());
    assert('\r'.isWhitespace());
    assert(!'\0'.isWhitespace());
}
