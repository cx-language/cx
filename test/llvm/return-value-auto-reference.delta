// RUN: check_matches_snapshot %delta -print-llvm %s -Wno-unused

var i = 42;

int* foo() {
    var a = A([false,true]).ff();
    var b = B('a').gg();
    return i;
}

struct A {
    bool[2] b;

    bool* ff() {
        return b[1];
    }
}

struct B: Copyable {
    char a;

    char* gg() {
        return a;
    }
}
