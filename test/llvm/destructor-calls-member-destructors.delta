// RUN: check_matches_snapshot %delta -print-llvm %s

extern bool f();

struct A {
    B b;
    B bb;
    int i;

    A() {
        b = undefined;
        bb = undefined;
        i = undefined;
    }

    ~A() {
        if (f()) {
            return;
        }
    }
}

struct B {
    int i;
    B() { i = 0; }
    ~B() {}
}

struct C {
    B b;
    B bb;
    C() {
        b = undefined;
        bb = undefined;
    }
}

void main() {
    var a = A();
    var c = C();
}
