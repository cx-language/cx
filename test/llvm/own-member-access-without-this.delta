// RUN: check_matches_snapshot %delta -print-llvm %s -w

struct A {
    int i
    int j

    A() {
        j = 42
    }

    int foo() {
        return j
    }

    void bar() {
        j = 1
    }
}

struct B: Copyable {
    int i
    int j

    B() {
        j = 42
    }

    int foo() {
        return j
    }

    void bar() {
        j = 1
    }
}

void main() {
    var a = A()
    a.foo()
    a.bar()
    var b = B()
    b.foo()
    b.bar()
}
