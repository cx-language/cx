// RUN: check_matches_snapshot %delta -print-llvm %s

enum E {
    A,
    B(bool b, int i),
    C(int j)
}

int main() {
    var e = E.A;
    e = E.B(b: false, i: 42);
    e = E.C(j: 43);

    switch (e) {
        case E.A: 
        case E.B as eb:
            e = E.C(j: eb.i);
            return eb.i;
        case E.C as eb:
            return eb.j;
    }

    _ = e == E.A;
    _ = e == E.B;

    return 0;
}
