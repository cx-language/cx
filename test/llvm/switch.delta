// RUN: check_matches_snapshot %delta -print-llvm %s -Wno-unused

void foo(int i) {
    switch i {
        case 0:
            break;
        case -1:
            foo(i);
            foo(i);
        case 42:
            break;
    }
    switch (i) {
        case 0:
            break;
        default:
            foo(i);
    }
}

struct S {
    int i;
}

int f(S* s) {
    switch s.i {
        case 0: return 0;
        default: return 1;
    }
}
