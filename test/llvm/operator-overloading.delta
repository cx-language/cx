// RUN: check_matches_snapshot %delta -print-llvm %s -Wno-unused

struct vec2: Copyable {
    int x
    int y

    int operator[](int index) {
        return index
    }
}

vec2 operator*(vec2 a, vec2 b) {
    return vec2(a.x * b.x, a.y * b.y)
}

void main() {
    var v = vec2(3, 2)
    v = vec2(2, 4) * v
    _ = v == vec2(-1, 3)
    _ = v[v.x]
}

bool operator==(vec2 a, vec2 b) {
    return a.x == b.x
}

struct A<T>: Copyable {}

bool operator==<T>(A<T> a, A<T> b) {
    return true
}
