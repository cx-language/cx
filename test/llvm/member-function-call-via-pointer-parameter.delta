// RUN: check_matches_snapshot %delta -print-llvm %s

struct Foo {
    int i;
    Foo() { i = 0; }
    void foo() { }
}

struct Bar: Copyable {
    int i;
    Bar() { i = 0; }
    void bar() { }
}

void qux(Foo* f, Bar* b) {
    f.foo();
    b.bar();
}

void main() {
    var f = Foo();
    var b = Bar();
    qux(f, b);
}
