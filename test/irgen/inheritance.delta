interface O {
    int p;
    int r;

    O(int p) {
        this.p = p;
        this.r = p;
    }

    void foo();
}

struct A : O {
    bool b;

    A(bool b, int p) {
        O.init(p);
        this.b = b;
    }

    void foo() {
        println("A");
    }
}

struct B : O {
    void foo() {
        println("B");
    }
}

