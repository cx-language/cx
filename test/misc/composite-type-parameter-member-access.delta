// RUN: check_exit_status 15 compile_and_run %s

struct vec2: Copyable {
    var x: int;
    var y: int;

    init(x: int, y: int) {
        this.x = x;
        this.y = y;
    }

    def add(that: vec2): vec2 {
        return vec2(this.x + that.x, this.y + that.y);
    }
}

def main(): int {
    var foo = vec2(5, 8);
    var foo2 = foo.add(vec2(3, -1));
    return foo2.x + foo2.y;
}
