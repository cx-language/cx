// RUN: %cx run %s | %FileCheck -match-full-lines -strict-whitespace %s
// CHECK:a
// CHECK-NEXT:c
// CHECK-NEXT:e
// CHECK-NEXT:g
// CHECK-NEXT:i
// CHECK-NEXT:k

void main() {
    if (1 == 1) { println("a"); }
    if (1 == 2) { println("b"); }
    if (0 != 1) { println("c"); }
    if (0 != 0) { println("d"); }
    if (1 <  2) { println("e"); }
    if (1 <  1) { println("f"); }
    if (1 >  0) { println("g"); }
    if (0 >  0) { println("h"); }
    if (0 <= 0) { println("i"); }
    if (0 <=-1) { println("j"); }
    if (0 >= 0) { println("k"); }
    if (0 >= 1) { println("l"); }
}
