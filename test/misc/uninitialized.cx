// RUN: check_exit_status 70 %cx run %s

int main() {
    uint8[4] buffer = undefined;
    buffer[0] = 70;
    buffer[1] = 0x6f;
    buffer[2] = 0o157;
    buffer[3] = 0b0;

    uint8 magic = undefined;
    if (buffer[0] == 0) {
        magic = 0;
    } else {
        magic = buffer[0];
    }
    return int(magic);
}
