extern def puts(s: uint8*): int;

def foo(): uint8[4] {
    var buffer: uint8[4] = undefined;
    buffer[0] = 66;
    buffer[1] = 0x61;
    buffer[2] = 0o162;
    buffer[3] = 0b0;
    return buffer;
}

def main() {
    var array = foo();
    puts(&array[0]);
}
