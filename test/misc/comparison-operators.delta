// RUN: compile_and_run %s | %FileCheck -match-full-lines -strict-whitespace %s
// CHECK:a
// CHECK-NEXT:c
// CHECK-NEXT:e
// CHECK-NEXT:g
// CHECK-NEXT:i
// CHECK-NEXT:k

def main() {
    if (1 == 1) { print("a"); }
    if (1 == 2) { print("b"); }
    if (0 != 1) { print("c"); }
    if (0 != 0) { print("d"); }
    if (1 <  2) { print("e"); }
    if (1 <  1) { print("f"); }
    if (1 >  0) { print("g"); }
    if (0 >  0) { print("h"); }
    if (0 <= 0) { print("i"); }
    if (0 <=-1) { print("j"); }
    if (0 >= 0) { print("k"); }
    if (0 >= 1) { print("l"); }
}
