// RUN: compile_and_run %s | %FileCheck -match-full-lines -strict-whitespace %s
// CHECK:a
// CHECK-NEXT:c
// CHECK-NEXT:e

def main() {
    if (true) {
        print("a");
    }

    if (false) {
        print("b");
    } else {
        print("c");
    }

    if (false) {
        print("d");
    } else if (true) {
        print("e");
    } else {
        print("d");
    }
}
