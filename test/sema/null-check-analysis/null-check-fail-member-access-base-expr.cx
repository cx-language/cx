// RUN: %cx -typecheck %s -Wno-unused | %FileCheck %s

struct S {
    S&? i;

    void f() {
        // CHECK: [[@LINE+1]]:17: warning: value may be null; unwrap it with a postfix '!' to silence this warning
        var j = i.i;

        if (i == null) {
            j = null;
        }
    }
}

void main() {
    var s = S(null);
    s.f();
}
