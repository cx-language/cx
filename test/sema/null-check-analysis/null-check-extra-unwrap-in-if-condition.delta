// RUN: %delta -typecheck %s | %FileCheck %s

struct S {
    S*? a;
}

S*? foo(S* p) {
    var n = p;

    while (n.a != null) {
        // CHECK: [[@LINE+1]]:21: warning: value cannot be null here; null check can be removed
        if (n == n.a!.a) {
            return n.a!.a;
        }
        n = n.a;
    }

    return null;
}
