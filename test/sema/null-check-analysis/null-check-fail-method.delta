// RUN: %delta -typecheck -Wno-unused %s

struct S {
    void f() {}
}

void foo(S*? a) {
    var p = a;

    // CHECK: [[@LINE+1]]:13: warning: receiver may be null; unwrap it with a postfix '!' to silence this warning
    p.f();
    // CHECK: [[@LINE+1]]:13: warning: receiver may be null; unwrap it with a postfix '!' to silence this warning
    a.f();
}
