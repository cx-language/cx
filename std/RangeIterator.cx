struct RangeIterator<T>: Copyable, Iterator<T> {
    T current;
    T end;

    RangeIterator(Range<T> range) {
        current = range.start();
        end = range.end();
    }

    bool hasValue() {
        return current != end;
    }

    T value() {
        return current;
    }

    void increment() {
        current++;
    }
}
